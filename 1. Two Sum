class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        // move from 1 to n
        int i = 0;
        int j = 1;
        while (j < nums.size())
        {
            if (nums[i] + nums[j] == target)
                return {i,j};
            else 
                {
                   ++i;
                   ++j;     
                }    
        }
        return {};
}
};
