class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        // move from 1 to n
        int i = 0;
        int j ;
        while (i < nums.size())
        {
            j = i+1;
            while (j < nums.size())
        {
            if (nums[i] + nums[j] == target)
                return {i,j};
            else 
                {
                  
                   ++j;     
                }    
        }
         ++i;
        }
        return {};
}
};
